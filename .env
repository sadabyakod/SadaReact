# API Configuration
# For development - using JSONPlaceholder (free testing API)
REACT_APP_API_URL=https://jsonplaceholder.typicode.com

# For production - replace with your actual API URL
# REACT_APP_API_URL=https://your-api-domain.com/api

# Optional: API Key if your endpoint requires authentication
# REACT_APP_API_KEY=your-api-key-here

# Azure Active Directory Configuration
REACT_APP_AZURE_CLIENT_ID=995d599d-cf8f-40b0-ac67-daa9b0b825ad

# Option 1: Use your specific tenant ID (GUID format)
 REACT_APP_AZURE_AUTHORITY=fcad4780-f7e9-412b-ba17-8ec9239b6fb2

# Option 2: Use your organization's domain name
# REACT_APP_AZURE_AUTHORITY=https://login.microsoftonline.com/yourorganization.onmicrosoft.com

# Option 3: Use common endpoint for multi-tenant (allows any Azure AD user)
REACT_APP_AZURE_AUTHORITY=https://login.microsoftonline.com/common

# Option 4: Use organizations endpoint (any organizational account)
#REACT_APP_AZURE_AUTHORITY=https://login.microsoftonline.com/organizations

REACT_APP_REDIRECT_URI=http://localhost:3000
